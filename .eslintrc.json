{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["webpack.common.js", "webpack.prod.js", "webpack.dev.js"],
    "rules": {
      "prettier/prettier": ["error", { "singleQuote": true}],
      "import/extensions": "off"
    }
}
